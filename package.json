{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:docs": "cd packages/custom-belt-lib && npm run build:docs",
    "build:ghp": "cd apps/docs && npm run build",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "prettier": "^2.5.1",
    "turbo": "^1.10.3"
  },
  "name": "custom-belt",
  "packageManager": "npm@8.9.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
