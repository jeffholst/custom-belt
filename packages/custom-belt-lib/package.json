    "eslint": "^8.41.0",
    "gulp": "^4.0.2",
    "typedoc": "^0.24.7",
    "typedoc-plugin-markdown": "^3.15.3",
    "typedoc-plugin-missing-exports": "^2.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-dts": "^2.3.0"
  },
  "dependencies": {
    "vitest": "^0.31.1"
  }
}
